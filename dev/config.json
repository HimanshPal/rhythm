{
    "api": {
        "auth": {
            "backend": "gitlab",
            "gitlab": {
                "baseURL": "https://gitlab.services.ams.osa"
            }
        }
    },
    "storage": {
        "backend": "zookeeper",
        "zookeeper": {
            "basePath": "/rhythm",
            "timeout": 10000,
            "auth": {
                "scheme": "digest",
                "digest": {
                    "user": "foo",
                    "password": "bar"
                }
            }
        }
    },
    "coordinator": {
        "backend": "zookeeper",
        "zookeeper": {
            "basePath": "/rhythm",
            "timeout": 10000,
            "auth": {
                "scheme": "digest",
                "digest": {
                    "user": "foo",
                    "password": "bar"
                }
            }
        }
    },
    "secrets": {
        "backend": "vault",
        "vault": {
            "token": "token",
            "address": "http://localhost:8200"
        }
    },
    "verbose": false,
    "mesos": {
        "labels": {
            "one": "1",
            "two": "2",
            "three": "3"
        },
        "principal": "foo",
        "auth": {
            "type": "basic",
            "basic": {
                "username": "foo",
                "password": "bar"
            }
        }
    }
}
